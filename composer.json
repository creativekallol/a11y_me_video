{
    "name": "iundd/a11y-me-video",
    "description": "Accessible video player based on MediaElement.js, A11y Plugin and Fluid. The new content elements support mp4, webm, subtitles, sign language video, audio description, poster image, text alternative, etc.",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "Video Player",
        "Video",
        "Accessibility",
        "Content Elements"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Robert Pudenz",
            "email": "opensource@i-d.de",
            "role": "Developer"
        },
        {
            "name": "Kallol Chakraborty",
            "email": "kchakraborty@learntube.de",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-backend": "^12.4",
        "typo3/cms-core": "^12.4",
        "typo3/cms-extbase": "^12.4",
        "typo3/cms-fluid": "^12.4",
        "typo3/cms-frontend": "^12.4"
    },
    "replace": {
        "typo3-ter/a11y-me-video": "2.0.0"
    },
    "autoload": {
        "psr-4": {
            "iundd\\A11yMeVideo\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "a11y_me_video"
        }
    }
}
